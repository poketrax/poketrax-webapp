{
  "name": "poketrax-webapp",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prepare": "npm run smui-theme-light && npm run smui-theme-dark",
    "smui-theme-light": "smui-theme compile src/assets/css/smui.css -i src/theme",
    "smui-theme-dark": "smui-theme compile src/assets/css/smui-dark.css -i src/theme/dark",
    "build": "vite build",
    "preview": "vite preview",
    "check": "svelte-check --tsconfig ./tsconfig.json"
  },
  "devDependencies": {
    "@material/elevation": "^13.0.0",
    "@sveltejs/vite-plugin-svelte": "^1.0.0-next.30",
    "@tsconfig/svelte": "^2.0.1",
    "autoprefixer": "^10.4.7",
    "postcss": "^8.4.14",
    "smui-theme": "^6.0.0-beta.16",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.7",
    "svelte-preprocess": "^4.9.8",
    "tailwindcss": "^3.1.5",
    "tslib": "^2.3.1",
    "typescript": "^4.5.4",
    "vite": "^2.9.9"
  },
  "dependencies": {
    "@mdi/js": "^7.0.96",
    "@smui-extra/autocomplete": "^6.0.0",
    "sass": "^1.53.0",
    "svelecte": "^3.9.1",
    "svelte-materialify": "^0.3.11"
  }
}
